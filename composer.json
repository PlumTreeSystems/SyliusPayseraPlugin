{
    "name": "plumtreesystems/sylius-paysera-plugin",
    "type": "sylius-plugin",
    "keywords": [
        "sylius",
        "sylius-plugin",
        "paysera",
        "payum"
    ],
    "description": "This is a sylius plugin that implements the plumtreesystems/payum-paysera-gateway",
    "homepage": "https://github.com/PlumTreeSystems/SyliusPayseraPlugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Mykolas Vitkus",
            "email": "mykolasvitkus@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "sylius/sylius": "^1.5.3",
        "plumtreesystems/payum-paysera-gateway": "^0.1.0"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "behat/mink": "^1.7@dev",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/symfony-extension": "^2.0",
        "friends-of-behat/variadic-extension": "^1.1",
        "friends-of-behat/page-object-extension": "^0.3",
        "lakion/mink-debug-extension": "^1.2.3",
        "phpspec/phpspec": "^5.0",
        "phpstan/phpstan-doctrine": "^0.10",
        "phpstan/phpstan-shim": "^0.10",
        "phpstan/phpstan-symfony": "^0.10",
        "phpstan/phpstan-webmozart-assert": "^0.10",
        "phpunit/phpunit": "^8.3",
        "sensiolabs/security-checker": "^5.0",
        "sylius-labs/coding-standard": "^3.0",
        "symfony/browser-kit": "^3.4|^4.1",
        "symfony/debug-bundle": "^3.4|^4.1",
        "symfony/dotenv": "^4.2",
        "symfony/intl": "^3.4|^4.1",
        "symfony/web-profiler-bundle": "^3.4|^4.1",
        "symfony/web-server-bundle": "^3.4|^4.1",
        "friends-of-behat/suite-settings-extension": "^1.0"
    },
    "prefer-stable": true,
    "scripts": {
        "run-spec": "./vendor/bin/phpspec run"
    },
    "autoload": {
        "psr-4": {
            "PTS\\SyliusPayseraPlugin\\": "src/",
            "Tests\\PTS\\SyliusPayseraPlugin\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/Application/Kernel.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}
